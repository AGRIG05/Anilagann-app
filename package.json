{
    "name": "anilagann",
    "version": "1.0.2",
    "description": "anilagann-app",
    "main": "./out/app.js",
    "author": "Alukkart",
    "homepage": "https://github.com/Alukkart/Anilagann-app",
    "scripts": {
      "format": "prettier --write .",
      "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
      "typecheck": "tsc --noEmit -p tsconfig.node.json --composite false",
      "start": "electron-vite preview",
      "dev": "electron-vite dev",
      "devl": "electron-vite dev --no-sandbox",
      "build": "npm run typecheck && electron-vite build",
      "postinstall": "electron-builder install-app-deps",
      "build:win": "npm run build && electron-builder --win --config",
      "build:mac": "npm run build && electron-builder --mac --config",
      "build:linux": "npm run build && electron-builder --linux --config"
    },
    "build": {
      "appId": "com.github.Alukkart.Anilagann-app",
      "productName": "Anilagann",
      "directories": {
        "buildResources": "build"
      },
      "nsis": {
        "installerIcon": "build/icon.ico",
        "installerHeaderIcon": "build/icon.ico",
        "deleteAppDataOnUninstall": true
      }
    },
    "dependencies": {
      "@cliqz/adblocker-electron": "^1.26.15",
      "@electron-toolkit/preload": "^3.0.0",
      "@electron-toolkit/utils": "^3.0.0",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "debug": "~2.6.9",
      "ejs": "^3.1.9",
      "electron-squirrel-startup": "^1.0.0",
      "electron-store": "^8.2.0",
      "electron-updater": "^6.1.7",
      "electron-vite": "^2.1.0",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.1",
      "helmet": "^7.0.0",
      "http-errors": "^1.6.3",
      "morgan": "^1.9.1",
      "node-fetch": "^2.7.0",
      "sqlite3": "^5.1.6"
    },
    "devDependencies": {
      "@electron-toolkit/tsconfig": "^1.0.1",
      "@rushstack/eslint-patch": "^1.1.4",
      "@types/cookie-parser": "^1.4.6",
      "@types/cors": "^2.8.17",
      "@types/http-errors": "^2.0.4",
      "@types/morgan": "^1.9.9",
      "@types/node-fetch": "^2.6.11",
      "@vitejs/plugin-vue": "^4.1.0",
      "@vue/eslint-config-prettier": "^7.0.0",
      "@vue/eslint-config-typescript": "^11.0.0",
      "eslint-plugin-vue": "^9.4.0",
      "less": "^4.1.3",
      "@electron-toolkit/eslint-config-prettier": "^2.0.0",
      "@electron-toolkit/eslint-config-ts": "^1.0.1",
      "@types/node": "^18.19.9",
      "electron": "^28.2.0",
      "electron-builder": "^24.9.1",
      "electron-vite": "^2.0.0",
      "eslint": "^8.56.0",
      "prettier": "^3.2.4",
      "typescript": "^5.3.3",
      "vite": "^5.0.12"
    }
  }
  